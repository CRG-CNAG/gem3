#!/bin/bash

echo "Total Maps (non-exact)"
cat $1 | awk '{print $5}' | tr ',' '\n' | awk -F ":" '{print $4}' | grep -v "100" | wc -l

echo "Repeated CIGARS"
cat $1 | awk '{print $5"\n>>>"}' | tr ',' '\n' | awk -F ":" '{if (NF>1) {print $4} else {print $0}}' | grep -v "100" | uniq -c | awk '{if ($1>1 && $2!=">>>") print}' | wc -l
